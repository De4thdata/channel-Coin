{
    "name": "Channel-Coin",
    "version": "1.1",
    "manifest_version": 2,
    "author": {
      "name": "Sami ZERRAI",
      "url": "https://github.com/SamiZerrai"
    },
    "description": "Automatically gather Channel points",
    "permissions": ["activeTab"],
    "content_scripts": [
      {
        "matches": ["*://*.twitch.tv/*"],
        "js": ["content.js"]
      }
    ],
    "icons":{
       "16":"asset/16.png",
       "32":"asset/32.png",
       "64":"asset/64.png"
    }
}